{
  "kind": "collectionType",
  "collectionName": "tasks",
  "info": {
    "singularName": "task",
    "pluralName": "tasks",
    "displayName": "Task",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "content-manager": {
      "visible": true
    }
  },
  "attributes": {
    "due_date": {
      "type": "date"
    },
    "assigned_to": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "name": {
      "type": "text"
    },
    "description": {
      "type": "text"
    },
    "contact": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::contact.contact"
    },
    "action": {
      "type": "text"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "planned",
        "in progress",
        "done",
        "expired"
      ]
    }
  }
}
