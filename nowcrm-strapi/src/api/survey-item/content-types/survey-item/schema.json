    },
    "contact": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::contact.contact",
      "inversedBy": "survey_items"
    },
    "videoask_option_id": {
      "type": "text"
    },
    "videoask_question_id": {
      "type": "string"
    },
    "file": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
