        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "date_published": {
      "type": "date"
    },
    "publication_type": {
      "type": "enumeration",
      "enum": [
        "journal",
        "newspaper",
        "scientific"
      ]
    },
    "contact": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::contact.contact",
      "inversedBy": "publications"
    }
  }
}
